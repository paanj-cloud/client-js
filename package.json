{
    "name": "@paanj/client",
    "version": "1.0.5",
    "description": "Core client SDK for Paanj platform - provides authentication and connection management",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/cjs/index.js",
            "import": "./dist/esm/index.js",
            "browser": "./dist/esm/index.js"
        }
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json",
        "build:cjs": "tsc -p tsconfig.cjs.json",
        "build:esm": "tsc -p tsconfig.esm.json",
        "watch": "tsc --watch",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "paanj",
        "client",
        "core",
        "sdk",
        "websocket",
        "authentication"
    ],
    "author": "Paanj",
    "license": "SEE LICENSE IN LICENSE",
    "repository": {
        "type": "git",
        "url": "https://github.com/paanj-cloud/client-js.git"
    },
    "homepage": "https://github.com/paanj-cloud/client-js#readme",
    "bugs": {
        "url": "https://github.com/paanj-cloud/client-js/issues"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "node-fetch": "^2.7.0",
        "ws": "^8.16.0"
    },
    "devDependencies": {
        "@types/node": "^20.19.25",
        "@types/node-fetch": "^2.6.13",
        "@types/ws": "^8.5.10",
        "typescript": "^5.0.0"
    },
    "browser": {
        "ws": false,
        "node-fetch": false
    }
}